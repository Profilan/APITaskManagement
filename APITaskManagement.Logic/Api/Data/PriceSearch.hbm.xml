<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="APITaskManagement.Logic.Api.Data"
                   assembly="APITaskManagement.Logic">
  <class name="PriceSearch" table="EEK_PRICESEARCH_API_RESPONSE">
    <id name="Id">
      <generator class="identity"/>
    </id>

    <property name="EAN" column="ean"/>
    <property name="LowestPrice" column="lowestPrice"/>
    <property name="BaseUrl" column="baseUrl"/>
    <property name="Price" column="price"/>
    <property name="Url" column="url"/>
    <property name="LastChecked" column="lastChecked" type="DateTime" not-null="true"/>
  </class>

</hibernate-mapping>