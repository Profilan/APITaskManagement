    <id name="Id">
      <generator class="guid.comb"/>
    </id>

    <property name="Rowtransactiontype"/>
    <property name="Itemcode"/>
    <property name="Prefix"/>
    <property name="Warehousecode"/>
    <property name="Warehousegroupcode"/>
    <property name="Period"/>
    <property name="ForecastAdjustmentType"/>
    <property name="Extractiondate"/>
    <property name="AdjustmentValueType"/>
    <property name="AdjustmentValue" precision="18" scale="2"/>
